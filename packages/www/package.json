{
  "name": "@try-svelte/www",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramram1048/try-svelte.git",
    "directory": "packages/www"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --loglevel silent --write ./src/**/*.{ts,svelte}",
    "lint": "eslint './src/**/*.{ts,svelte}'",
    "lint:css": "stylelint './src/**/*.svelte'",
